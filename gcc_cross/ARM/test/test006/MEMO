ARM の gcc のスタックフレーム

f:
	mov	ip, sp
	stmfd	sp!, {fp, ip, lr, pc}
	sub	fp, ip, #4

        +--------------+ <- sp_{new}
        |   fp_{old}   |
        +--------------+ +4
        |   sp_{old}   |
        +--------------+ +8
        |   lr_{old}   |
        +--------------+ <- fp_{new}
        |   pc_{old}+8 |
        +--------------+ <- sp_{old}

