(*2)
Z8000 は printf にスタックで引数を渡すのだが, ここで push 命令を生成したい.
現状 main 函数のコードは以下:

_main:
	push	@r15, r10
	ld	r10, r15
	call	___main
	ld	r6, #6
	ld	r7, #5
	call	_f
	ld	r3, r2
	r15 := r15 + -4
	ld	r2, r15
	(mem:HI (plus:HI (reg:HI 2 r2 [27])
        (const_int 2 [0x2])) [0  S2 A8]) := r3
	(mem:HI (reg:HI 2 r2 [27]) [0  S2 A8]) := _LC0
	call	_printf
	r15 := r15 + 4
	ld	r2, #0
	ld	r15, r10
	pop	r10, @r15
	ret	t

