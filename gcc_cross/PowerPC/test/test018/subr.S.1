	.machine ppc
	.section	".text"
	.align 2
	.globl f
f:
	stwu 1,-32(1)		/* (r1 := r1 - 32, [r1 - 32] = r1) */
	mflr 0			/* r0 := lr */
	stw  0,0(1)		/* [r1] := r0 */
	stw 31,4(1)		/* [r1+4] := r31 */
	mr 31,1			/* r31 := r1 */
	stw 3,8(31)		/* [r31+8] := r3 */
	stw 4,12(31)		/* [r31+12] := r4 */
	
	lwz 10,8(31)		/* r10 : = [r31+8] */
	lwz 9,12(31)		/* r9 := [r31+12] */
	add 9,10,9		/* r9 := r10 + r9 */
	mr 3,9			/* r3 := r9 */
	
	lwz 0,0(1)		/* r0 := [r1] */
	mtlr 0			/* lr := r0 */
	lwz 31,4(1)		/* r1 := [r1 + 28] */
	addi 1,1,32		/* r1 := r1 + 32 */
	blr			/* pc := lr */

